<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- CYBERPUNK THEME: Electric neon matrix - ISOLATED BRUSH VOCABULARY -->
    
    <!-- === CYBER THEME EXCLUSIVE BRUSHES - NO SHARING === -->
    
    <!-- Cyber Background Hierarchy -->
    <SolidColorBrush x:Key="CyberBaseBrush" Color="#0a0a0f"/>          <!-- Deep space black -->
    <SolidColorBrush x:Key="CyberPanelBrush" Color="#1a0f1f"/>        <!-- Purple-tinted dark -->
    <SolidColorBrush x:Key="CyberInputBrush" Color="#2a1a2f"/>        <!-- Purple input backgrounds -->
    
    <!-- Cyber Button System -->
    <SolidColorBrush x:Key="CyberButtonBrush" Color="#2a2a3a"/>       <!-- Dark button base -->
    <SolidColorBrush x:Key="CyberButtonHoverBrush" Color="#333344"/>  <!-- Hover state -->
    <SolidColorBrush x:Key="CyberButtonPressedBrush" Color="#3d3d55"/><!-- Pressed state -->
    <SolidColorBrush x:Key="CyberButtonBorderBrush" Color="#7c3aed"/> <!-- Electric purple borders -->
    
    <!-- Cyber Primary Accent (Electric magenta) -->
    <SolidColorBrush x:Key="CyberAccentBrush" Color="#ff00ff"/>       <!-- Electric magenta -->
    <SolidColorBrush x:Key="CyberAccentHoverBrush" Color="#e600e6"/>  <!-- Hover magenta -->
    <SolidColorBrush x:Key="CyberAccentPressedBrush" Color="#cc00cc"/><!-- Pressed magenta -->
    
    <!-- Cyber Border System -->
    <SolidColorBrush x:Key="CyberBorderBrush" Color="#7c3aed"/>       <!-- Electric purple borders -->
    <SolidColorBrush x:Key="CyberBorderHoverBrush" Color="#8b5cf6"/>  <!-- Bright purple hover -->
    
    <!-- Cyber Text System -->
    <SolidColorBrush x:Key="CyberPrimaryTextBrush" Color="#00ffff"/>  <!-- Cyan for dark backgrounds -->
    <SolidColorBrush x:Key="CyberSecondaryTextBrush" Color="#00ff00"/><!-- Matrix green secondary -->
    <SolidColorBrush x:Key="CyberMutedTextBrush" Color="#e0e7ff"/>    <!-- Bright purple-white -->
    <SolidColorBrush x:Key="CyberDisabledTextBrush" Color="#6b46c1"/> <!-- Dim purple disabled -->
    
    <!-- Cyber Button Text System -->
    <SolidColorBrush x:Key="CyberButtonTextBrush" Color="#e0e7ff"/>   <!-- Light purple-white for dark buttons -->
    <SolidColorBrush x:Key="CyberButtonTextHoverBrush" Color="#ffffff"/><!-- Bright white on hover -->
    <SolidColorBrush x:Key="CyberAccentTextBrush" Color="#000000"/>   <!-- Black text for bright magenta -->
    
    <!-- Cyber Status Colors -->
    <SolidColorBrush x:Key="CyberSuccessBrush" Color="#00ff00"/>      <!-- Matrix green success -->
    <SolidColorBrush x:Key="CyberErrorBrush" Color="#ff1493"/>        <!-- Hot pink error -->
    <SolidColorBrush x:Key="CyberWarningBrush" Color="#ffff00"/>      <!-- Electric yellow warning -->
    
    <!-- Cyber Control System -->
    <SolidColorBrush x:Key="CyberControlBrush" Color="#111117"/>      <!-- Dark input backgrounds -->
    <SolidColorBrush x:Key="CyberControlHoverBrush" Color="#1a1a22"/> <!-- Hover state -->
    <SolidColorBrush x:Key="CyberControlFocusBrush" Color="#202028"/> <!-- Focus state -->
    
    <!-- Cyber Dropdown System -->
    <SolidColorBrush x:Key="CyberDropdownBrush" Color="#2a1a2f"/>     <!-- Dark purple dropdown -->
    <SolidColorBrush x:Key="CyberDropdownTextBrush" Color="#00ffff"/> <!-- Cyan text in dropdowns -->
    <SolidColorBrush x:Key="CyberDropdownHoverBrush" Color="#ff00ff"/><!-- Magenta hover background -->
    <SolidColorBrush x:Key="CyberDropdownHoverTextBrush" Color="#000000"/><!-- Black text on bright hover -->
    <SolidColorBrush x:Key="CyberDropdownSelectedTextBrush" Color="#000000"/><!-- Black text selected -->
    
    <!-- Cyber Alternative Accents -->
    <SolidColorBrush x:Key="CyberPurpleBrush" Color="#a855f7"/>       <!-- Electric purple -->
    <SolidColorBrush x:Key="CyberPurpleHoverBrush" Color="#8b5cf6"/>  <!-- Bright purple hover -->
    <SolidColorBrush x:Key="CyberPurplePressedBrush" Color="#7c3aed"/><!-- Deep purple pressed -->
    
    <SolidColorBrush x:Key="CyberGreenBrush" Color="#00ff00"/>        <!-- Matrix green -->
    <SolidColorBrush x:Key="CyberGreenHoverBrush" Color="#00e600"/>   <!-- Bright green hover -->
    <SolidColorBrush x:Key="CyberGreenPressedBrush" Color="#00cc00"/> <!-- Dark green pressed -->
    
    <SolidColorBrush x:Key="CyberYellowBrush" Color="#ffff00"/>       <!-- Electric yellow -->
    <SolidColorBrush x:Key="CyberYellowHoverBrush" Color="#e6e600"/>  <!-- Bright yellow hover -->
    <SolidColorBrush x:Key="CyberYellowPressedBrush" Color="#cccc00"/><!-- Dark yellow pressed -->
    
    <SolidColorBrush x:Key="CyberBlueBrush" Color="#00ffff"/>         <!-- Cyan blue -->
    <SolidColorBrush x:Key="CyberBlueHoverBrush" Color="#00e6e6"/>    <!-- Bright cyan hover -->
    <SolidColorBrush x:Key="CyberBluePressedBrush" Color="#00cccc"/>  <!-- Dark cyan pressed -->
    
    <!-- === SEMANTIC BRUSH MAPPING FOR CYBER THEME === -->
    
    <!-- Background Mapping -->
    <SolidColorBrush x:Key="PrimaryDarkBrush" Color="#0a0a0f"/>
    <SolidColorBrush x:Key="SecondaryDarkBrush" Color="#1a0f1f"/>
    <SolidColorBrush x:Key="TertiaryDarkBrush" Color="#2a1a2f"/>
    
    <!-- Button Mapping -->
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="#2a2a3a"/>
    <SolidColorBrush x:Key="ButtonHoverBrush" Color="#333344"/>
    <SolidColorBrush x:Key="ButtonPressedBrush" Color="#3d3d55"/>
    
    <!-- Primary Button Mapping -->
    <SolidColorBrush x:Key="AccentRedBrush" Color="#ff00ff"/>
    <SolidColorBrush x:Key="AccentRedHoverBrush" Color="#e600e6"/>
    <SolidColorBrush x:Key="AccentRedPressedBrush" Color="#cc00cc"/>
    
    <!-- Border Mapping -->
    <SolidColorBrush x:Key="BorderBrush" Color="#7c3aed"/>
    <SolidColorBrush x:Key="BorderHoverBrush" Color="#8b5cf6"/>
    
    <!-- Control Mapping -->
    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="#111117"/>
    <SolidColorBrush x:Key="ControlHoverBrush" Color="#1a1a22"/>
    <SolidColorBrush x:Key="ControlFocusBrush" Color="#202028"/>
    
    <!-- Text Mapping -->
    <SolidColorBrush x:Key="TextPrimaryBrush" Color="#00ffff"/>    <!-- CRITICAL FIX: Cyan text on dark background -->
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="#00ff00"/>
    <SolidColorBrush x:Key="TextMutedBrush" Color="#e0e7ff"/>
    <SolidColorBrush x:Key="TextDisabledBrush" Color="#6b46c1"/>
    <SolidColorBrush x:Key="GeneralTextBrush" Color="#00ffff"/>
    
    <!-- Button Text Mapping -->
    <SolidColorBrush x:Key="ButtonTextNormalBrush" Color="#e0e7ff"/>
    <SolidColorBrush x:Key="ButtonTextHoverBrush" Color="#ffffff"/>
    <SolidColorBrush x:Key="ButtonTextBrush" Color="#000000"/>    <!-- Black text for bright magenta buttons -->
    
    <!-- Status Mapping -->
    <SolidColorBrush x:Key="SuccessBrush" Color="#00ff00"/>
    <SolidColorBrush x:Key="ErrorBrush" Color="#ff1493"/>
    <SolidColorBrush x:Key="WarningBrush" Color="#ffff00"/>
    
    <!-- Dropdown Mapping -->
    <SolidColorBrush x:Key="DropdownTextBrush" Color="#00ffff"/>
    <SolidColorBrush x:Key="DropdownTextHoverBrush" Color="#000000"/>
    <SolidColorBrush x:Key="DropdownTextSelectedBrush" Color="#000000"/>
    
    <!-- Alternative Accent Mapping -->
    <SolidColorBrush x:Key="AccentPurpleBrush" Color="#a855f7"/>
    <SolidColorBrush x:Key="AccentPurpleHoverBrush" Color="#8b5cf6"/>
    <SolidColorBrush x:Key="AccentPurplePressedBrush" Color="#7c3aed"/>
    
    <SolidColorBrush x:Key="AccentGreenBrush" Color="#00ff00"/>
    <SolidColorBrush x:Key="AccentGreenHoverBrush" Color="#00e600"/>
    <SolidColorBrush x:Key="AccentGreenPressedBrush" Color="#00cc00"/>
    
    <SolidColorBrush x:Key="AccentYellowBrush" Color="#ffff00"/>
    <SolidColorBrush x:Key="AccentYellowHoverBrush" Color="#e6e600"/>
    <SolidColorBrush x:Key="AccentYellowPressedBrush" Color="#cccc00"/>
    
    <SolidColorBrush x:Key="AccentBlueBrush" Color="#00ffff"/>
    <SolidColorBrush x:Key="AccentBlueHoverBrush" Color="#00e6e6"/>
    <SolidColorBrush x:Key="AccentBluePressedBrush" Color="#00cccc"/>
    
    <!-- Error Background -->
    <SolidColorBrush x:Key="ErrorBackgroundBrush" Color="#2a0a1a"/>
    
    <!-- Utility -->
    <SolidColorBrush x:Key="TransparentBrush" Color="Transparent"/>

</ResourceDictionary>